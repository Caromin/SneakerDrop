@{
    ViewData["Title"] = "Catalog";
}
@using (Html.BeginForm("Catalog", "Home", FormMethod.Get))
{
<div class="search d-inline">
 
     <div>
            <main role="main">
                <div class="homeBanner2"></div>
                <div class="container  mt-5 box-shadow">
                    <h1>Search Results</h1>
                    <div class="d-flex align-content-start flex-wrap">
                        @foreach (var item in ViewData)
                        {
                            
                                  
                                <span class="d-block productText">@item.Key</span>
                                <img class="productImage" src="@item.Value"/>
                               
                        }
                    </div>
                </div>
            </main>
        </div>
    
        
    
</div>
}